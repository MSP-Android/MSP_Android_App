<?xml version="1.0" encoding="utf-8"?>

<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:id="@+id/card_project_item"
    android:layout_height="wrap_content"
    android:layout_marginTop="10dp"
    app:cardBackgroundColor="#E7EDF6"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/Projectimage"
        android:layout_width="130dp"
        android:layout_height="100dp"
        android:translationZ="120dp"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/recyshape2"
        android:src="@drawable/mspgray" />

    <androidx.cardview.widget.CardView

        android:layout_width="320dp"
        android:layout_height="170dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="30dp"
        app:backgroundTint="@null"
        android:background="@drawable/web_custom"
        android:orientation="vertical"
        app:cardCornerRadius="30dp"
        >
        <TextView
            android:paddingTop="50dp"
            android:id="@+id/ProjectName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:text="MSP APP"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginVertical="50dp"
            android:layout_marginLeft="20dp"
            android:paddingTop="50dp"
            android:text="Asdasdasd asd as dasdasd ad asdsa"
            android:textColor="@color/black"
            android:textSize="16dp" />


    </androidx.cardview.widget.CardView>

</androidx.cardview.widget.CardView>